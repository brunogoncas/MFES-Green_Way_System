--	generic entry
--	for things like mcdrive, gas, etc.

class GenericEntry

instance variables

	private val : real;

operations

	public GenericEntry : real ==> GenericEntry
	
		GenericEntry(value) == (val := value; return self)
	
		pre value >= 0
		post val = value;
	
	-- Getters
	
	public getValue: () ==> real
		getValue() == return val;

end GenericEntry