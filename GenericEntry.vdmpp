--	generic entry
--	for things like mcdrive, gas, etc.

class GenericEntry is subclass of Entry

instance variables

	private val : real;

operations

	public GenericEntry : real ==> GenericEntry
	
		GenericEntry(value) == (val := value; return self)
	
		pre value >= 0
		post val = value;
	
	-- Getters
	
	public pure getValueToPay: Vehicle ==> real
		getValueToPay(v) == return val;	--	How do I supress this?

end GenericEntry